<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>storage-scanner@iris-advies.com</em:id>
		<em:name>Storage Scanner Plugin for Zotero</em:name>
		<em:version>0.0.5</em:version>
		<em:description>A sample plugin for Zotero</em:description>
		<em:creator>Center for History and New Media<br/>George Mason University</em:creator>
		<em:updateURL>https://raw.github.com/friflaj/zotero-storage-scanner/master/update.rdf</em:updateURL>
		<em:type>2</em:type> <!-- type=extension -->
		
		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>3.0</em:minVersion>
				<em:maxVersion>4.0.*</em:maxVersion>
			</Description>
		</em:requires>
    
		<!-- Requires Firefox or Zotero Standalone -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>17.0</em:minVersion>
				<em:maxVersion>23.*</em:maxVersion>
			</Description>
		</em:targetApplication>
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>3.0</em:minVersion>
				<em:maxVersion>4.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
